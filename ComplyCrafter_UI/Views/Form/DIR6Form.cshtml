@section Scripts {
    <script>
        $(document).on("change", "#directorDin", function () {
            let dinName = $("#directorDin option:selected").text(); 
            $("#directorName").val(dinName);
        });

        $(".directorinfo").on("change", function () {
            // Construct corresponding input ID by appending '_input'
            var inputId = "#" + this.id + "_input";
            if ($(this).is(":checked")) {
                $(inputId).removeAttr("readonly");
            } else {
                // Add the readonly attribute when unchecked
                $(inputId).attr("readonly", true);
            }
        });

        var frm = {
            id: 0,
            companyId: 0,

            directorDin: "", // This will store the selected DIN/DPIN value

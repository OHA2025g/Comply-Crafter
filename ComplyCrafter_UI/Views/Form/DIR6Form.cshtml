            isResidentInIndia: "",  // Stores if director is Resident In India
            dob: "",       // Stores the date of birth
            gender: "",    // Stores the gender (male, female, transgender)
            panNumber: "",          // Income-tax permanent account number
            panAttachment: "",      // Income tax PAN attachment
            aadhaarNumber: "",      // Aadhaar number
            aadhaarAttachment: "",  // Aadhaar attachment
            voterIdNumber: "",      // Voter's identity card number
            voterIdAttachment: "",  // Voter's identity card attachment
            passportNumber: "",     // Passport number
            passportAttachment: "", // Passport attachment
            drivingLicenseNumber: "", // Driving license number
            drivingLicenseAttachment: "", // Driving license attachment
            
            permanentAddressLine1: "",

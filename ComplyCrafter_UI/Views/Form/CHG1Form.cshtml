<script>
    document.addEventListener('DOMContentLoaded', function () {
        const plotRadio = document.getElementById('plotRadio');
        const dwellingRadio = document.getElementById('dwellingRadio');
        const commonFields = document.getElementById('commonFields');
        const plotSection = document.getElementById('plotSection');
        const dwellingSection = document.getElementById('dwellingSection');
        const part13Section = document.getElementById('part13Section'); // Wrap for radio + fields

        const immovableCheckboxes = document.querySelectorAll(
            'input[name="chkInterest"], input[name="chkCommercial"], input[name="chkInterestThereIn"]'
        );

        plotRadio.checked = false;
        dwellingRadio.checked = false;

        function updateView() {
            const anyCheckboxSelected = Array.from(immovableCheckboxes).some(cb => cb.checked);

            // If no immovable checkbox is checked, hide the whole radio + fields part
            if (!anyCheckboxSelected) {
                part13Section.style.display = 'none';
                plotRadio.checked = false;
                dwellingRadio.checked = false;
                commonFields.style.display = 'none';
                plotSection.style.display = 'none';
                dwellingSection.style.display = 'none';
                return;
            }

            // Show section if checkbox is selected
            part13Section.style.display = 'block';

            const selected = document.querySelector('input[name="unitType"]:checked');
            if (!selected) {
                commonFields.style.display = 'block';
                plotSection.style.display = 'none';
                dwellingSection.style.display = 'none';
            } else if (selected.value === 'plot') {
                commonFields.style.display = 'none';
                plotSection.style.display = 'block';
                dwellingSection.style.display = 'none';
            } else if (selected.value === 'dwelling') {
                commonFields.style.display = 'none';
                plotSection.style.display = 'none';
                dwellingSection.style.display = 'block';
            }
        }

        // Listen to changes on checkboxes & radios
        immovableCheckboxes.forEach(cb => cb.addEventListener('change', updateView));
        document.querySelectorAll('input[name="unitType"]').forEach(r => r.addEventListener('change', updateView));

        // Initial run
        updateView();
    });
</script>


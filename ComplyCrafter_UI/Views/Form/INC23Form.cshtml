<script>
    $(document).ready(function () {
        $(".fillig_purpose").on("change", function () {
            const selected = $(this).val();

            if (selected === "Change in State within the jurisdiction of existing ROC") {
                $(".point10-section, .point11-section").show();
            } else {
                $(".point10-section, .point11-section").hide();
            }
        });
    });

    function addRow(tableId) {
        const table = document.getElementById(tableId);
        const rowCount = table.rows.length;
        const row = table.insertRow(rowCount);
        const firstRow = table.rows[0];

        for (let i = 0; i < firstRow.cells.length; i++) {
            const newCell = row.insertCell(i);
            if (i === 0) {
                newCell.innerHTML = rowCount + 1;
            } else {
                newCell.innerHTML = firstRow.cells[i].innerHTML;
                const input = newCell.querySelector('input, select');
                if (input) input.value = '';
            }
        }
    }

    function deleteRow(tableId) {
        const table = document.getElementById(tableId);
        const rowCount = table.rows.length;
        if (rowCount > 1) {
            table.deleteRow(rowCount - 1);
        }
    }
</script>



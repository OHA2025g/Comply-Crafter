        states.forEach(state => {
            $stateDropdown.append(`<option value="${state}">${state}</option>`);
        });

      
        $stateDropdown.on("change", function () {
            const selectedState = $(this).val();
            $rocDropdown.empty().append('<option value="">Select ROC</option>');

            if (rocMap[selectedState]) {
                rocMap[selectedState].forEach(roc => {
                    $rocDropdown.append(`<option value="${roc}">${roc}</option>`);
                });
            }
        });
    });

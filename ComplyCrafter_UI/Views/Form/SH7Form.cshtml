            designation: "",      // Stores the selected designation
            designation2: "", // Stores the selected designation
            identifier: "",   // Stores the DIN, PAN, or Membership number

            category: "",
            pcs: "",
            membershipStatus: "",
            membershipNumber: ""
        }


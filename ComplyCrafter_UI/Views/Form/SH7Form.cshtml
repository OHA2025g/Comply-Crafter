            id: 0,
            parentId: 0,
            shareType: "",         
            classOfShares: "",             
            authNumberOfShares: 0,         
            authNominalValue: 0,           
            authTotalAmount: 0,            
            issuedNumberOfShares: 0,       
            issuedNominalValue: 0,         
            issuedTotalAmount: 0,          
            subscribedNumberOfShares: 0,   
            subscribedNominalValue: 0,     
            subscribedTotalAmount: 0,      
            paidUpNumberOfShares: 0,       
            paidUpNominalValue: 0,         
            paidUpTotalAmount: 0           
        };

        $(document).ready(function () {

            $("#part2").hide();
            $("#part3").hide();
            $('a[data-bs-toggle="tab"]').on('shown.bs.tab', function (e) {
                var active = $(e.target).attr('role');
                $("#part1").hide();
                $("#part2").hide();
                $("#part3").hide();
                $("#" + active).show();

            });

            load()
            vfFrm = $('#frm').validate({ rules: voFrm, onsubmit: () => { console.log('submit') } })

        })

        function save(event) {
            event.preventDefault();

            // let isValid = true;
            // if (!vfFrm1.form()) isValid = false;

            // if (!isValid) return;

            var obj = getObj(frm, '#frm');

            obj.shares = $.makeArray($('#shareTable tbody tr')).map(tr => getObj(tbl1, tr));

            var existingTable = [];
            $('#existingTable tbody tr').each(function () {
                d = getObj(tbl2, this)
                existingTable.push(d);
            });
            obj.existingStructure = existingTable;

            var revisedTable = [];
            $('#revisedTable tbody tr').each(function () {
                d = getObj(tbl2, this)
                revisedTable.push(d);
            });
            obj.revisedStructure = revisedTable;

            $._post(`@R.ApiUrl/@ViewBag.ctrl`, obj).then(x => {
                if (x.status) {
                    window.location = `@Url.Action("", "Form")/@ViewBag.ctrl`
                } else {
                    swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: x.message
                    })
                }
            })

        }

        function load() {
            if (@ViewBag.id > 0) {
                $._get(`@R.ApiUrl/@ViewBag.ctrl/${@ViewBag.id}`).then(x => {
                    frm = x
                    setObj(x, "#frm")
                    if (x.shares.length > 0) {
                        x.shares.forEach(function () {
                            shareTableAdd()
                        });
                        $('#shareTable tbody tr').each(function (index) {
                            setObj(x.shares[index], this);
                        });
                    } else {
                        shareTableAdd()
                    }


                    if (x.existingStructure.length > 0) {
                        x.existingStructure.forEach(function () {
                            structureTableAdd("existing")
                        });
                        $('#existingTable tbody tr').each(function (index) {
                            setObj(x.existingStructure[index], this);
                        });
                    } else {
                        structureTableAdd("existing")
                    }

                    if (x.revisedStructure.length > 0) {
                        x.revisedStructure.forEach(function () {
                            structureTableAdd("revised")
                        });
                        $('#revisedTable tbody tr').each(function (index) {
                            setObj(x.revisedStructure[index], this);
                        });
                    } else {
                        structureTableAdd("revised")
                    }
                })
            } else {
                shareTableAdd()
                structureTableAdd("existing")
                structureTableAdd("revised")
            }
            var company = localStorage.getItem("comp_id");
            if (company === null || company === undefined || company === "0") {
                alert("Please select Company")
                window.location = `@Url.Action("", "Form")/@ViewBag.ctrl`
                return;
            }
            $._get(`@R.ApiUrl/Company/${company} `).then(x => {
                $("[name='companyId']").val(x.id);
                $("[name='cin']").val(x.cin);
                $("[name='companyName']").val(x.companyName);
                $("[name='companyAddress']").val(x.address);
                $("[name='companyEmail']").val(x.email);
            })

        }


            id: 0,
            parentId: 0,
            typeOfShares: "",  // Default value is empty string
            classOfShares: "",  // Default value is empty string
            nominalValuePerShare: 0,  // Default value is 0
            existingNumberOfShares: 0,  // Default value is 0
            revisionRequired: "",  // Default value is empty string
            additionalNumberOfShares: 0,  // Default value is 0
            revisedNumberOfShares: 0,  // Default value is 0
            existingTotalValue: 0,  // Default value is 0
            revisedTotalValue: 0,  // Default value is 0
            totalAddition: 0,  // Default value is 0
            briefDescriptionOfConditions: ""  // Default value is empty string
        };


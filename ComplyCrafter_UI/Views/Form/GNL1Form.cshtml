                    </div>
                    <div class="col-xs-12 col-sm-3 col-md-3 form-row mt-3">
                        <label>   &nbsp;<input type="radio" name="associateOrFellow" class="" value="Associate">&nbsp; Associate&nbsp;</label>
                        <label>  <input type="radio" name="associateOrFellow" value="Fellow">&nbsp;Fellow&nbsp;</label>
                    </div>

                    <div class="col-xs-12 col-sm-3 col-md-3 form-row bold CERT_PRACTICE_NO1 mt-3" style="">
                        Membership number
                    </div>
                    <div class="col-xs-12 col-sm-3 col-md-3 form-row CERT_PRACTICE_NO1 mt-3" style="">
                        <input type="text" name="membershipNumber" class="form-control membership num" id="membershipnum" value="">
                    </div>
                </div>

                <div class="row CERT_PRACTICE_NO mt-3" style="margin-bottom: 0px;">
                    <div class="col-xs-12 col-sm-3 col-md-3 form-row bold mt-4">
                        Certificate of practice number
                    </div>
                    <div class="col-xs-12 col-sm-3 col-md-3 form-row mt-3">
                        <input type="text" name="certPractiseNumber" class="form-control certpractisenum" id="certpractisenum" value="">
                    </div>
                </div><br>
            </form>
        </div>
    </div>

    <div class="col-m-12 mt-3">
        <button class="btn btn-md btn-primary" style="float: right;" onclick="save(event)">
            Submit
        </button>
    </div>
</div>

@section Scripts {
    <script>

        var frm = {
            id: 0,
            companyId: 0,

            cin: "",
            companyName: "",
            companyAddress: "",
            companyEmail: "",

            // Part 1 fields
            applicantCategory: "",
            CorporateGln: "",
            name: "",
            addressLine1: "",
            addressLine2: "",
            city: "",
            state: "",
            isoCode: "",
            country: "",
            pinCode: "",
            email: "",
            filedFor: "",
            others: "",
            applicationDetails: "",

            // Part 2 fields
            filedCompany: "",
            filedDirector: "",
            filedManager: "",
            filedOther: "",
            personsFiled: "",
            applicationFiled: "",
            noticeNumber: 0,
            sectionFiled: "",
            particularsBrief: "",
            agmDate: "",
            srnMgt14: "",
            datePassing: "",
            mgt14Date: "",
            stampDutyAmount: 0,

            // Part 3 fields
            directorAuthorization: "",
            resolutionNo: "",
            resolutionDate: "",
            selfDeclaration: "",
            designation: "",
            declaration: "",
            din: "",
            digitallySignedBy: "",
            nameOfPcs: "",
            associateOrFellow: "",
            membershipNumber: 0,
            certPractiseNumber: ""
        }


        $(document).on("change", "#applicantCategory",function(){
            let category = $(this).val()
            if (category == "Others") {
                $(".othersdetails").show()
            }
            else if (category=="Company") {
                $(".companyContainer").show()
            }
            else if (category == "Foreign Companies") {

            }
        })

        $(document).on("change", "#filedFor",function(){
            let val= $(this).val()
            if (val == "Compounding of offences"){
                $(".offenceContainer").show()
            }
            else if (val == 'Extension of period of annual general meeting by three months'){
                $(".extensionContainer").show()
            }
           
        })

        $(document).on("change", "[name='associateOrFellow']", function () {
            if ($(this).val() == "Company") {
                $("#noofpersons_filed").hide()

            }else{
                $("#noofpersons_filed").show()
            }
        })

      

        var voFrm = {
            id: 'required',
            companyId: 'required',
        };

        var vfFrm = null;

        $(document).ready(function () {

            $("#part2").hide();
            $("#part3").hide();
            $('a[data-bs-toggle="tab"]').on('shown.bs.tab', function (e) {
                var active = $(e.target).attr('role');
                $("#part1").hide();
                $("#part2").hide();
                $("#part3").hide();
                $("#" + active).show();

            });

            load()
            vfFrm = $('#frm').validate({ rules: voFrm, onsubmit: () => { console.log('submit') } })

            function togglePersonsFiledRow() {
                const companyChecked = $('input[name="filedCompany"]').is(':checked');
                const directorChecked = $('input[name="filedDirector"]').is(':checked');
                const managerChecked = $('input[name="filedManager"]').is(':checked');
                const otherChecked = $('input[name="filedOther"]').is(':checked');

                if (companyChecked && !(directorChecked || managerChecked || otherChecked)) {
                    $('#personsFiledRow').hide();
                }
                else if (directorChecked || managerChecked || otherChecked) {
                    $('#personsFiledRow').show();
                }
                else {
                    $('#personsFiledRow').hide();
                }
            }

            togglePersonsFiledRow();

            $('input[name="filedCompany"], input[name="filedDirector"], input[name="filedManager"], input[name="filedOther"]').on('change', togglePersonsFiledRow);

        })


        function toggleCertificationFields() {
            var selectedValue = $('input[name="digitallySignedBy"]:checked').val();
            if (selectedValue === "Company secretary (in whole-time practice)") {
                $('.CERT_PRACTICE_NO1, .CERT_PRACTICE_NO').show();
            } else {
                $('.CERT_PRACTICE_NO1, .CERT_PRACTICE_NO').hide();
            }
        }

        $('.CERT_PRACTICE_NO1, .CERT_PRACTICE_NO').hide();

        $('input[name="digitallySignedBy"]').on('change', toggleCertificationFields);

        $(document).on("change", "[name='applicationFiled']",function(){
            let appFiled = $(this).val()
            if (appFiled == "In pursuance to notice received from RoC or any other competent authority") {
                $(".noticeanddate").show()
            }else{
                $(".noticeanddate").hide()
            }
        })


        function save(event) {
            event.preventDefault();

            // let isValid = true;
            // if (!vfFrm1.form()) isValid = false;

            // if (!isValid) return;

            var obj = getObj(frm, '#frm');
            $._post(`@R.ApiUrl/@ViewBag.ctrl`, obj).then(x => {
                console.log(x)
                if (x.status) {
                    window.location = `@Url.Action("", "Form")/@ViewBag.ctrl`
                } else {
                    swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: x.message
                    })
                }
            })

        }

        function load() {
            if (@ViewBag.id > 0) {
                $._get(`@R.ApiUrl/@ViewBag.ctrl/${@ViewBag.id}`).then(x => {
                    frm = x
                    setObj(x, "#frm")
                })
            }
            var company = localStorage.getItem("comp_id");
            if (company === null || company === undefined || company === "0") {
                alert("Please select Company")
                window.location = `@Url.Action("", "Form")/@ViewBag.ctrl`
                return;
            }
            $._get(`@R.ApiUrl/Company/${company} `).then(x => {
                $("[name='companyId']").val(x.id);
                $("[name='cin']").val(x.cin);
                $("[name='companyName']").val(x.companyName);
                $("[name='companyAddress']").val(x.address);
                $("[name='companyEmail']").val(x.email);
            })

        }


    </script>

}
<script>


    $(document).on("change", "[name='formPurpose']", function () {
    const selectedValue = $(this).val();

    if (selectedValue === "Particulars of transactions by a company not considered as deposit as per rule 2(1)(c) of the Companies (Acceptance of Deposit) Rules, 2014") {
      
        $("[name='depositAccepted']").prop("disabled", true).prop("checked", false);
    } else {
      
        $("[name='depositAccepted']").prop("disabled", false);
    }
});


        const signatoryDropdownMap = {
        "Director": {
                    api:"https://dev-api.complycrafter.com/api/Director/ByCompany/",
            dropdownId: "#signatoryId_director",
            labelKeys: ["firstName", "lastName"],
            valueKey: "din"
        },
        "Manager": {
                    api: "https://dev-api.complycrafter.com/api/Director/ByCompany/",
            dropdownId: "#signatoryId_manager",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "CEO": {
            api: "/api/ceo?companyId=",
            dropdownId: "#signatoryId_ceo",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "CFO": {
            api: "/api/cfo?companyId=",
            dropdownId: "#signatoryId_cfo",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "Company Secretary": {
            api: "/api/secretary?companyId=",
            dropdownId: "#signatoryId_cs",
            labelKeys: ["name"],
            valueKey: "membershipNumber"
        }
    };


    $(document).on("change", "[name='signatoryDesignation']", function () {
    const designation = $(this).val();
    const companyId = $("[name='companyId']").val();

    
    $("[name='signatoryId']").show().val(""); 

   
    if (signatoryDropdownMap[designation]) {
        const config = signatoryDropdownMap[designation];
        const apiUrl = config.api + companyId;

       
        let $dropdown = $(config.dropdownId);
        if (!$dropdown.length) {
            $dropdown = $('<select class="form-select" name="signatoryId" style="box-shadow: 0 0 11px 1px rgba(0, 0, 0, .05);"></select>')
                .attr("id", config.dropdownId.replace("#", ""));
            $("[name='signatoryId']").after($dropdown); 
        }

        $("[name='signatoryId']").hide(); 
        $dropdown.empty().show().append(`<option value="">Select ${designation}</option>`);

        $.getJSON(apiUrl, function (data) {
            if (Array.isArray(data) && data.length > 0) {
                data.forEach(function (item) {
                    const nameParts = config.labelKeys.map(k => item[k] || "").filter(Boolean);
                    const fullName = nameParts.join(" ");
                    const value = item[config.valueKey] || "";
                    if (value) {
                        $dropdown.append(
                            $("<option></option>").val(value).text(`${fullName} (${value})`)
                        );
                    }
                });
            } else {
                $dropdown.append('<option disabled>No records found</option>');
            }
        }).fail(function () {
            $dropdown.append('<option disabled>Error fetching data</option>');
        });
    } else {
        
        $("[id^='signatoryId_']").remove();
    }
});








        $(document).on("change", "input[name='depositAccepted']", function () {
        const selectedValue = $(this).val();

        if (selectedValue === "yes") {
           
            $(".depositeReturn").show();
            $("#dateOfIssue, #dateOfExpiry").prop("disabled", false);
        } else if (selectedValue === "no") {
           
            $(".depositeReturn").show();
            $("#dateOfIssue, #dateOfExpiry").prop("disabled", true).val(""); 
        }
    });








           $(document).on("change", "#returnPeriod, #dateOfIssue, #dateOfExpiry, [name='creditRatingDate'], #declarationDate", function () {
        const selectedDate = new Date($(this).val());
        const currentDate = new Date();
        const days120Ago = new Date();
        days120Ago.setDate(currentDate.getDate() - 120);

        let error = '';

        if (isIncorpDateLoaded && dateOfIncorporation && selectedDate < dateOfIncorporation) {
            error = "Date cannot be before Date of Incorporation.";
        }
        else if (
            $(this).attr("id") === "returnPeriod" && selectedDate < days120Ago
        ) {
            error = "Date cannot be more than 120 days older than today.";
        }

        if (error) {
            $(this).addClass("is-invalid");
            $(this).next(".invalid-feedback").text(error).show();
        } else {
            $(this).removeClass("is-invalid");
            $(this).next(".invalid-feedback").text("").hide();
        }
    });


</script>




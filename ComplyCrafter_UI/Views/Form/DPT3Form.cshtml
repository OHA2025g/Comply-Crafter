            $('a[data-bs-toggle="tab"]').on('shown.bs.tab', function (e) {
                var active = $(e.target).attr('role');
                $("#part1").hide();
                $("#part2").hide();
                $("#part3").hide();
                $("#" + active).show();

            });
            $('input').on('input', function () {
                // Get values from the input fields
                var paidUpShareCapital = parseFloat($('#paidUpShareCapital').val()) || 0;
                var freeReserves = parseFloat($('#freeReserves').val()) || 0;
                var securitiesPremiumAccount = parseFloat($('#securitiesPremiumAccount').val()) || 0;
                var accumulatedLoss = parseFloat($('#accumulatedLoss').val()) || 0;
                var deferredRevenueExpenditureBalance = parseFloat($('#deferredRevenueExpenditureBalance').val()) || 0;
                var accumulatedUnprovidedDepreciation = parseFloat($('#accumulatedUnprovidedDepreciation').val()) || 0;
                var miscellaneousExpense = parseFloat($('#miscellaneousExpense').val()) || 0;
                var otherIntangibleAssets = parseFloat($('#otherIntangibleAssets').val()) || 0;

                // Calculate Net Worth (A) - (B)
                var netWorth = (paidUpShareCapital + freeReserves + securitiesPremiumAccount) - (accumulatedLoss + deferredRevenueExpenditureBalance + accumulatedUnprovidedDepreciation + miscellaneousExpense + otherIntangibleAssets);
                $('#netWorth').val(netWorth.toFixed(2)); // Display Net Worth

                // Calculate Maximum Deposit Limit (35% of Net Worth)
                var depositMaxLimit = netWorth * 0.35;
                $('#depositMaxLimit').val(depositMaxLimit.toFixed(2)); // Display Maximum Deposit Limit
            });


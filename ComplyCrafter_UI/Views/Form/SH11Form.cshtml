            const container = $(containerId);
            container.empty();

            for (let i = 1; i <= count; i++) {
                container.append('<h6 class="mt-4 mb-2 fw-bold text-primary">Class ' + i + '</h6>');
                container.append(getCapitalTableHTML(shareType));
            }
        });
    }


<script>
   
    const dropdownMap = {
        "Director": {
            api: "https://dev-api.complycrafter.com/api/Director/ByCompany/",
            dropdownId: "#drpDirector",
            labelKeys: ["firstName", "lastName"],
            valueKey: "din"
        },
        "Manager": {
            api: "https://dev-api.complycrafter.com/api/Director/ByCompany/",
            dropdownId: "#drpManager",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "CEO": {
            api: "/api/ceo?companyId=",
            dropdownId: "#drpCeo",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "CFO": {
            api: "/api/cfo?companyId=",
            dropdownId: "#drpCfo",
            labelKeys: ["name"],
            valueKey: "pan"
        },
        "Company Secretary": {
            api: "/api/secretary?companyId=",
            dropdownId: "#drpCompanySecretary",
            labelKeys: ["name"],
            valueKey: "membershipNumber"
        },
        "Designated Partner": {
            api: "/api/partner?companyId=",
            dropdownId: "#drpPartner",
            labelKeys: ["name"],
            valueKey: "din"
        }
    };

    $(document).on("change", "#designation", function () {
        const designation = $(this).val();
        const companyId = $("[name='companyId']").val() || 0;

       
        $("#drpDirector, #drpManager, #drpCeo, #drpCfo, #drpCompanySecretary, #drpPartner").hide().empty();

        if (dropdownMap[designation]) {
            const config = dropdownMap[designation];
            const apiUrl = config.api + companyId;
            const $targetDropdown = $(config.dropdownId);

            $targetDropdown.show().append(`<option value="">Select ${designation}</option>`);

            $.getJSON(apiUrl, function (data) {
                if (Array.isArray(data) && data.length > 0) {
                    data.forEach(item => {
                        const label = config.labelKeys.map(k => item[k] || "").filter(Boolean).join(" ");
                        const value = item[config.valueKey];
                        if (value) {
                            $targetDropdown.append(
                                $("<option></option>").val(value).text(`${label} (${value})`)
                            );
                        }
                    });
                } else {
                    $targetDropdown.append('<option disabled>No records found</option>');
                }
            }).fail(function () {
                $targetDropdown.append('<option disabled>Error fetching data</option>');
            });
        }
    });


    

    function updateVerificationFields() {
        const applicantFirst = $("input[name='applicantFirstName']").val();
        const applicantMiddle = $("input[name='applicantMiddleName']").val();
        const applicantLast = $("input[name='applicantLastName']").val();
        const fullName = [applicantFirst, applicantMiddle, applicantLast].filter(Boolean).join(" ");

        const fatherFirst = $("input[name='fatherFirstName']").val();
        const fatherMiddle = $("input[name='fatherMiddleName']").val();
        const fatherLast = $("input[name='fatherLastName']").val();
        const fatherFullName = [fatherFirst, fatherMiddle, fatherLast].filter(Boolean).join(" ");

        const dob = $("input[name='dateOfBirth']").val();
        const nationality = $("select[name='nationality']").val();

        if (fullName) {
            $("#verificationName").val(fullName).prop("readonly", true);
        }

        if (fatherFullName) {
            $("#verificationFathersName").val(fatherFullName).prop("readonly", true);
        }

        if (dob) {
            $("#verificationDob").val(dob).prop("readonly", true);
        }

        if (nationality) {
            $("#verificationAddress").val(nationality).prop("readonly", true);
        }
    }

    $(document).ready(function () {
        $("input[name^='applicant'], input[name^='father'], input[name='dateOfBirth'], select[name='nationality']")
            .on("input change blur", updateVerificationFields);
    });










          $(document).on("change", "[name='pinCode']", function () {
        const pin = $(this).val().trim();

        if (pin.length !== 6 || isNaN(pin)) {
            $(this).addClass("is-invalid");
            $(this).next(".invalid-feedback").text("Please enter a valid 6-digit PIN code").show();
            return;
        } else {
            $(this).removeClass("is-invalid").next(".invalid-feedback").text("").hide();
        }

        $.get(`@R.ApiUrl/Company/PinCode/${pin}`)
            .then(response => {
                if (response && response[0]?.PostOffice?.length > 0) {
                    const info = response[0].PostOffice[0];

                    $("[name='country']").val(info.Country || 'India');
                    $("[name='stateUT']").val(info.State || '');
                    $("[name='city']").val(info.Region || '');
                    $("[name='district']").val(info.District || '');

                    // Handle areaLocality as dropdown if multiple values exist
                    const areaField = $("[name='areaLocality']");
                    if (areaField.is("select")) {
                        areaField.empty().append(`<option value="">Select Locality</option>`);
                        response[0].PostOffice.forEach(postOffice => {
                            areaField.append(`<option value="${postOffice.Name}">${postOffice.Name}</option>`);
                        });
                    } else {
                        // Replace input with a dropdown if multiple post offices are returned
                        if (response[0].PostOffice.length > 1) {
                            const select = $('<select class="form-select" name="areaLocality" style="box-shadow: 0 0 11px 1px rgba(0, 0, 0, .05);"></select>');
                            select.append(`<option value="">Select Locality</option>`);
                            response[0].PostOffice.forEach(postOffice => {
                                select.append(`<option value="${postOffice.Name}">${postOffice.Name}</option>`);
                            });
                            areaField.replaceWith(select);
                        } else {
                            areaField.val(info.Name || '');
                        }
                    }
                } else {
                    Swal.fire({
                        icon: 'warning',
                        title: 'No Data Found',
                        text: 'Could not find area details for this PIN code.'
                    });
                }
            })
            .catch(() => {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Something went wrong while fetching PIN code details.'
                });
            });
    });


</script>

